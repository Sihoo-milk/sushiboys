<section class="section">
  <ul class="discography">
    <li class="discography__item">
      <h2 class="title">Ashtray</h2>
      <div id="swiper-ashtray" class="swiper discography__swiper">
        <ul class="swiper-wrapper discography__swiper-list">
          <% let ashtrays = goodsData.ashtrays; %>
          <% ashtrays.forEach(function(ashtray) { %>
          <li class="swiper-slide discography__card">
            <a href="<%- ashtray.url %>" target="_blank" rel="noopener noreferrer">
              <figure class="discography__frame">
                <img class="discography__image" src="<%- ashtray.image %>" alt="<%- ashtray.alt %>">
              </figure>
            </a>
          </li>
          <% }); %>
        </ul>
      </div>
    </li>
